datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model  Book{
  id    Int     @default(autoincrement()) @id
  title String  
  author  String
  publisher  String
  year Int
  description String
  ISBN String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}